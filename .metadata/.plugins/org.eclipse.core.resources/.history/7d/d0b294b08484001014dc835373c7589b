<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sample.mapper.ProductMapper">
	
	<select id="categoryList" resultType="java.util.HashMap">
		SELECT CATE_CODE
			 , CATE_NAME
		  FROM CATEGORY
	</select>
	
	<select id="productList" resultType="java.util.HashMap">
		SELECT CATE_CODE
			, PRODUCT_NAME
			, PRODUCT_PRICE
			, PRODUCT_STOCK
			, PRODUCT_CONTENT
				FROM PRODUCT
	</select>
	
</mapper>
